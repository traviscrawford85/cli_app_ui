{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Clio Textual App",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONBREAKPOINT": "pdb.set_trace"
      },
      "justMyCode": false
    }
  ]
}
