# Auto-generated x-tagGroups for OpenAPI extension
{% import "helpers/tag_groups.jinja" as tag_helpers %}
{{ tag_helpers.render_tag_groups(tag_groups) }}

x-tagGroups:
{% for group in tag_groups %}
  - name: "{{ group.name }}"
    tags:
    {% for tag in group.tags %}
      - "{{ tag }}"
    {% endfor %}
{% endfor %}